{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "target": "ES2017",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "isolatedModules": false,
    "resolveJsonModule": true
  }
}
